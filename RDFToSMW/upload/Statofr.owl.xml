[obo:IAO_0000119 xml:lang="fr"]https://fr.wikipedia.org/wiki/Analyse_de_survie[/obo:IAO_0000119]
[obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/obi.owl"/]
[ont:ISI_Glossary xml:lang="fr"]http://isi.cbs.nl/glossary/term3246.htm[/ont:ISI_Glossary]
